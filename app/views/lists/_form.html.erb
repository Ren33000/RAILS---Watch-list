<div class="form-container" id="my-form">
  <div class="form-styled">
    <h1>New List</h1>
    <div class="row">
      <div class="col-8">
        <%= simple_form_for (@list) do |f| %>
          <%= f.input :name, placeholder: "What is your list's name?"%>
          <%= f.input :photo, as: :file %>
          <%= f.submit class: "btn btn-danger" %>
        <% end %>
      </div>
    </div>
    </div>
</div>